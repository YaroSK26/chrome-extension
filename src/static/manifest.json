{
  "manifest_version": 3,
  "name": "Hello ReactJS Extensions",
  "description": "React Extension",
  "version": "1.0.0",
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "bookmarks"]
}
